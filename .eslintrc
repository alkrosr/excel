{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },

  "rules": {
    "semi": "off",
    "quotes": "off",
    "indent": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "max-len": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
