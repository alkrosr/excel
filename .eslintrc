{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "semi": "off",
    "quotes": "off",
    "indent": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error"
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:jest/recommended"
  ]
}
